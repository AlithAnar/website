{
  "name": "minarets_website",
  "description": "A community website for Dave Matthews Band fans to interact and listen to fan taped show archives.",
  "version": "0.1.0",
  "private": true,
  "author": "Jim Geurts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minarets/website/issues"
  },
  "browserslist": [
    "last 2 major versions and supports es6-module-dynamic-import and not < 0.5% and not dead",
    "last 2 firefox version",
    "last 2 chrome version",
    "last 2 edge version",
    "ios_saf >= 13.4",
    "safari >= 13.1"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint  --fix --ext .ts,.tsx api components pages",
    "lint-staged": "lint-staged",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "bootstrap": "5.0.0-alpha2",
    "cross-fetch": "3.0.6",
    "ioredis": "4.19.2",
    "moment": "2.29.1",
    "next": "10.0.1",
    "next-auth": "3.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "1.29.0",
    "smartystreets-javascript-sdk": "1.6.0",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@types/ioredis": "^4.17.7",
    "@types/next-auth": "^3.1.14",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/smartystreets-javascript-sdk": "^1.5.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vercel/node": "^1.8.3",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^5.0.0-alpha.6",
    "lint-staged": "^10.4.0",
    "pinst": "^2.0.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
