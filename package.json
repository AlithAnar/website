{
  "name": "minarets_website",
  "description": "A community website for Dave Matthews Band fans to interact and listen to fan taped show archives.",
  "version": "0.1.0",
  "private": true,
  "author": "Jim Geurts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minarets/website/issues"
  },
  "browserslist": [
    "last 2 major versions and supports es6-module-dynamic-import and not < 0.5% and not dead",
    "last 2 firefox version",
    "last 2 chrome version",
    "last 2 edge version",
    "ios_saf >= 13.4",
    "safari >= 13.1"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint  --fix --ext .ts,.tsx api components pages",
    "lint:staged": "lint-staged",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@sentry/browser": "6.2.5",
    "@sentry/integrations": "6.2.5",
    "@sentry/node": "6.2.5",
    "@sentry/tracing": "6.2.5",
    "@sentry/webpack-plugin": "1.14.2",
    "algoliasearch": "4.8.6",
    "bootstrap": "^5.0.0-beta3",
    "cross-fetch": "3.1.4",
    "domutils": "2.5.1",
    "htmlparser2": "6.0.1",
    "ioredis": "4.25.0",
    "moment": "2.29.1",
    "next": "10.1.3",
    "next-auth": "3.13.3",
    "react": "17.0.2",
    "react-autosuggest": "10.1.0",
    "react-content-loader": "6.0.2",
    "react-dom": "17.0.2",
    "react-hotkeys-hook": "3.3.0",
    "react-instantsearch-dom": "6.10.3",
    "react-slider": "1.1.4",
    "sass": "1.32.8",
    "smartystreets-javascript-sdk": "1.7.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@types/ioredis": "^4.22.2",
    "@types/next-auth": "^3.13.0",
    "@types/react": "^17.0.3",
    "@types/react-autosuggest": "^10.1.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-instantsearch-core": "^6.10.0",
    "@types/react-instantsearch-dom": "^6.10.0",
    "@types/react-slider": "^1.1.1",
    "@types/smartystreets-javascript-sdk": "^1.6.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vercel/node": "^1.9.1",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.4.0",
    "pinst": "^2.1.6",
    "prettier": "^2.1.2",
    "typescript": "^4.2.3"
  }
